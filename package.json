{
  "name": "@tn/baithuocnamhay.service",
  "version": "0.0.1",
  "description": "BaiThuocNamHayService",
  "main": "./bin/src/index.js",
  "scripts": {
    "start": "yarn start:prod",
    "start:prod": "yarn env:prod node ./bin/src/index.js",
    "start:stage": "yarn env:stage node ./bin/src/index.js",
    "start:dev": "yarn build:tsoa && yarn env:dev ts-node-dev --respawn ./src",
    "build": "yarn build:tsoa && tsc && yarn copy:swagger",
    "build:routes": "tsoa routes",
    "build:swagger": "tsoa swagger",
    "build:tsoa": "yarn clean && yarn build:routes && yarn build:swagger",
    "copy:swagger": "cp -R ./build/swagger ./bin/build/swagger",
    "clean": "vnrm bin",
    "env:dev": "cross-env NODE_ENV=development",
    "env:stage": "cross-env NODE_ENV=stage",
    "env:prod": "cross-env NODE_ENV=production",
    "lint": "tslint ./src/**/*.ts"
  },
  "author": "Tinh Ngo",
  "license": "MIT",
  "dependencies": {
    "body-parser": "^1.18.3",
    "dotenv": "^5.0.1",
    "express": "^4.16.4",
    "inversify": "^4.11.1",
    "inversify-binding-decorators": "^3.2.0",
    "method-override": "^3.0.0",
    "mongoose": "^5.0.12",
    "mongoose-unique-validator": "^2.0.0",
    "morgan": "^1.9.1",
    "multer": "^1.3.0",
    "reflect-metadata": "^0.1.12",
    "swagger-ui-express": "^4.0.1",
    "tsoa": "^2.3.0",
    "uuid": "^3.2.1",
    "winston": "^3.0.0-rc4",
    "cross-env": "^5.1.4"
  },
  "devDependencies": {
    "@types/body-parser": "^1.17.0",
    "@types/express": "^4.16.0",
    "@types/method-override": "0.0.31",
    "@types/mongoose": "^5.0.2",
    "@types/mongoose-unique-validator": "^1.0.1",
    "ncp": "^2.0.0",
    "ts-node-dev": "^1.0.0-pre.31",
    "typescript": "^3.1.6",
    "vnrm": "^0.0.11"
  }
}
